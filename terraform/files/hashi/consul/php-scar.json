{
  "service": {
    "name": "php",
    "port": 80,
    "connect": {
      "sidecar_service": {
        "proxy": {
          "upstreams": [
            {
              "destination_name": "mysql",
              "local_bind_port": 5000
            }
          ]
        }
      }
    },
    "check": {
      "id": "php-check",
      "tcp": "localhost:80",
      "interval": "10s",
      "timeout": "1s"
    }
  }
}
